QUESTION LINK:-
https://leetcode.com/problems/move-zeroes/description/

SOLUTION 1:-

class Solution 
{
public:
    void moveZeroes(vector<int>& nums) 
    {
        int lastNonZeroIndex = 0; 
        for (int i = 0; i < nums.size(); i++)
        {
            if (nums[i] != 0)
            {
                swap(nums[i], nums[lastNonZeroIndex]); 
                lastNonZeroIndex++; 
            }
        }
    }
};



SOLUTION 2:-

#include <vector>
class Solution 
{
public:
    void moveZeroes(vector<int>& nums) 
    {
        vector<int> t;
        for (int i=0; i<nums.size(); i++)
        {
            if(nums[i]!=0)
            {
                t.push_back(nums[i]);
            }
        }
        int nz = t.size();
        for(int i=0;i<nz;i++)
        {
            nums[i]=t[i];
        }
        for(int i=nz;i<nums.size();i++)
        {
            nums[i]=0;
        }   
    }
};
